{
  "version": 2,
  "name": "crm-backend-shopify",
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/**"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ]
}
